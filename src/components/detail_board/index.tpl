<div class="detail-board cover">
  <div class="container">
    <div class="img-wrap">
      <img src="{{pic}}" alt="{{name}}" />
    </div>
    <div class="info">
      <h3 class="name">{{name}}</h3>
      <p class="slogan">{{slogan}}</p>
      <p class="price">{{price}}元起</p>
      {{title_1}}
      <div class="content-wrap clearfix">
        {{version_list}}
      </div>
      {{title_2}}
      <div class="content-wrap clearfix">
        {{color_list}}
      </div>
    </div>
  </div>
</div>